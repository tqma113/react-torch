    "conventional-changelog": "^3.1.23",
    "conventional-changelog-cli": "^2.1.0",
    "css-loader": "^5.0.1",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "lint-staged": "^10.2.13",
    "mini-css-extract-plugin": "^1.3.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.1.1",
    "pretty-quick": "^3.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "redux": "^4.0.5",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.4.1",
    "typescript": "^4.2.4"
  }
}
