    "@types/cross-spawn": "^6.0.2",
    "@types/debug": "^4.1.5",
    "@types/detect-port": "^1.3.0",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.1",
    "@types/helmet": "0.0.45",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^24.0.19",
    "@types/mini-css-extract-plugin": "^1.2.2",
