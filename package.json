    "commander": "^6.2.0",
    "compression": "^1.7.4",
    "content-disposition": "^0.5.3",
    "cookie-parser": "^1.4.4",
    "cookies": "^0.8.0",
    "cross-spawn": "^7.0.3",
    "debug": "^4.1.1",
    "detect-port": "^1.3.0",
    "esbuild": "^0.11.12",
    "esbuild-loader": "^2.12.0",
    "express": "^4.17.1",
    "farrow-http": "^1.10.3",
    "farrow-pipeline": "^1.10.2",
    "farrow-react": "^1.10.3",
    "farrow-schema": "^1.10.3",
    "fork-ts-checker-webpack-plugin": "^6.2.0",
    "fs-extra": "^9.0.1",
    "helmet": "^3.21.3",
    "inquirer": "^7.3.3",
    "is-root": "^2.1.0",
    "leven": "^3.1.0",
    "mime-types": "^2.1.31",
    "morgan": "^1.9.1",
    "null-loader": "^4.0.0",
    "open": "^7.2.1",
    "redux-logger": "^3.0.6",
    "serve-favicon": "^2.5.0",
    "tiny-invariant": "^1.1.0",
    "tiny-warning": "^1.0.3",
    "torch-history": "^1.0.1",
    "torch-router": "^1.1.2",
